{
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "eslint.validate": ["typescript", "typescriptreact", "javascript", "javascriptreact"],
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.useAliasesForRenames": false,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.eol": "\n",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.localesPaths": ["packages/lib/server-only/i18n"],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#bfee9d",
    "activityBar.background": "#bfee9d",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#5c95e3",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#bfee9d",
    "statusBar.background": "#a2e771",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#85e045",
    "statusBarItem.remoteBackground": "#a2e771",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#a2e771",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a2e77199",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#A2E771"
}
